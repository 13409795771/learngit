/* abt.wxss */
.abt-container {
    background-color: #e1e8ee;
    width: 100%;
    height: 100%;
    position:absolute;
}

.abt-item {
  background-color: white;
  margin:30rpx;
  padding:10rpx;
  border-width: 2rpx;
  border-radius: 10rpx;
}

.abt-item-title {
  font-size:34rpx;
  width:100%;
  color:#86939e;
  border-bottom-color: #e1e8ee;
  border-bottom-width: 10rpx;
}

.abt-item-content {
  color:#86939e;
  font-size:38rpx;
}